{
    "name": "Password Encoder",
    "description": "Provides a highly secure password from a simple password",
    "version": "1.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'sha256-z5xxzCAJrlfsx6TD3afwye1zOpC2iLoyxteoZ/WTaac='; object-src 'self'",
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "logo1.png"
      },
      "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" },

    "permissions": [
        "tabs"
    ]
  }